<% content_for :left_header do -%>
   New contact group
<%end-%>

<table class ='form-table'>
<% form_for([:admin,@contact_group]) do |f| %>
  <%= f.error_messages %>

  <tr>
    <th>Name</th>
    <td><%= f.text_field :name %></td>
  </tr>

  <tr>
   <td></td>
    <td><%= f.submit 'Create' %> | <%= link_to 'Back', admin_contact_groups_path %></td>
  </tr>

<% end %>
</table>

