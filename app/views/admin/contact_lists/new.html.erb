<h2>New contact list</h2><br/>

<table>
<% form_for([:admin,@contact_list]) do |f| %>
  <%= f.error_messages %>

  <tr>
    <th>Select group id</th>
    <td><%= f.select :contact_group_id, all_contact_groups, options={:prompt => 'Group id'}%></td>
  </tr>

 <tr>
    <th>Name</th>
    <td><%= f.text_field :name %></td>
  </tr>

  <tr>
    <th>Number</th>
    <td><%= f.text_field :number %></td>
  </tr>

  <tr>
    <td></td>
    <td><%= f.submit 'Update' %></td>
  </tr>
<% end %>

</table>

<%= link_to 'Back', admin_contact_lists_path %>
