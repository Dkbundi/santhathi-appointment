<% content_for :sidebar do -%>
  <div class="search1">
    <p> <%= remote_per_page_select -%>   </p>
    <% form_remote_tag(:url => admin_masters_parameters_url, :method => :get) do %>
      <%fields_for @search do |f|%>
        <% f.fields_for @search.conditions do |p| %>
          <p> <label> Parameter Name: </label> <%= p.text_field :parameter_name_like, :class => 'textbox'%> </p>
          <p> <label> Value Type: </label> <%= p.text_field :value_type_like , :class => 'textbox-small'%> </p>
          <% p.fields_for p.object.measurement_unit do |mu| %>
              <p> <label> Measurement Unit: </label> <%= mu.text_field :name_like , :class => 'textbox-small'%> </p>
          <%end%>
        <%end%>
      <%end%>
      <input type="reset" value="Reset"> &nbsp; &nbsp;  <%= submit_tag "Search" %>
    <% end %>
  </div>
<%end%>

