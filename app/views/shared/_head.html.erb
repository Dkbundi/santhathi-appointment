 <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
   <title><%= configatron.site_name %></title>
   <%= stylesheet_link_tag 'css/smoothness/jquery-ui-1.7.1.custom.css', 'css/superfish.css', 'facebox' %>
   <%= stylesheet_link_tag('css/style.css', :media => :screen) %>
   <%= stylesheet_link_tag('print', :media => :print) %>

   <!--[if IE 7]><link rel="stylesheet" href="/css/ie.css" type="text/css" media="screen, projection" /><![endif]-->
   <!--[if IE 6]><link rel="stylesheet" href="/css/ie6.css" type="text/css" media="screen, projection" /><![endif]-->
   <!-- JavaScript -->
   <%=javascript_include_tag  'js/jquery-1.3.2.min.js', 'jquery_corner', 'js/jquery-ui-1.7.1.custom.min.js', 'jrails', 'facebox', 'js/hoverIntent.js', 'js/superfish.js' %>
   <script type="text/javascript">
      
      jQuery(document).ready(function(){
         jQuery("#datepicker").datepicker({ altField: '#date', altFormat: 'dd MM, yy', onSelect: function(dateText, inst) { jQuery('#spinner').show(); jQuery.ajax({data:'date=' + dateText +'&doctor=' + jQuery('#appointment_doctor_id').val() + '&authenticity_token=' + encodeURIComponent("<%= escape_javascript form_authenticity_token %>"), dataType:'script', success:function(request){jQuery('#spinner').hide();}, type:'get', url:"<%= escape_javascript new_pms_appointment_url %>"}) }
});
      });
   </script>
   
