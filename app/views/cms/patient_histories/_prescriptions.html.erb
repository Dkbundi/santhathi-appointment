<table class="listing">
<tr> <th> Date </th>
     <th> Prescribed by</th>
     <th> Service </th>
     <th> Remarks </th>
</tr>

 <% unless @prescribed_tests.blank? %>
      <% for test in @prescribed_tests %>
        <tr>
           <td> <%=h Time.parse(test.created_at.to_s).strftime('%m/%d/%Y') %></td>
           <td> <%=h @appointment.doctor.doctor_profile.name %></td>
           <td> <%=h test.service.name %></td>
           <td> <%=h test.prescription.remarks  %></td>
        </tr>
       <%end%>
   <%else%>
       Prescription is not found.
   <%end%>

</table>
