<div class='continer'>
  <div class='continer-inner'>
     <% form_for :patient,:url=>patients_path do |f| %>
       <%= f.error_messages %>
       <%= render :partial => @partial, :locals => {:f => f} %>
       <p>
         <%= f.submit "Create" %>
       </p>
     <% end %>

     <%= link_to 'Back', patients_path %>
   </div>
</div>
