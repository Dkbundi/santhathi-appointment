<div class='continer'>
  <div class='continer-inner'>
  <label> <h3> Parmeter Details </h3></label><br/>

    <table class="form-table">
       <tr> <th>Patient Age : </th><td>  <%=h calculate_age(@patient.dob)%> </td> </tr>
       <tr> <th>Patient Gender : </th><td>  <%=h @patient.gender %> </td> </tr>
       <tr> <th>Parameter Name : </th><td>  <%=h @parameter_specification.parameter.parameter_name %> </td> </tr>
